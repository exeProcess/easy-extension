{
    "manifest_version": 3,
    "name": "Easy notes",
    "description": "Easy notes Launcher",
    "version": "1.0",
    "icons": {"128": "images/icon1.png"},
    "action": {
        "default_icon": "images/icon1.png",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "css": ["style.css","bootstrap.min.css"],
          "js": ["jquery.min.js","bootstrap.bundle.min.js"]
        }
    ],
    "permissions": ["storage","scripting", "activeTab"]
}